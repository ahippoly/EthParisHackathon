{
  "name": "harpos-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --ignore-path .gitignore",
    "lint:fix": "yarn lint --fix",
    "env:select": "bash ./scripts/set_env.bash",
    "build:local": "yarn env:select development && nest build",
    "build:staging": "yarn env:select staging && nest build",
    "build:prod": "yarn env:select production && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:staging": "yarn build:staging && yarn run:staging",
    "start:prod": "yarn build:prod && yarn run:prod",
    "run:staging": "cross-env NODE_ENV=staging && node dist/src/main",
    "run:prod": "cross-env NODE_ENV=production && node dist/src/main",
    "db:dev": "yarn env:select development && docker-compose --env-file envs/.env up -d harpos-dev-database",
    "db:prod": "yarn env:select production && docker-compose --env-file envs/.env up -d harpos-database"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/mongoose": "^9.2.1",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schedule": "^2.2.2",
    "@nestjs/swagger": "^6.3.0",
    "@sismo-core/sismo-connect-server": "^0.0.20",
    "@typegoose/typegoose": "^10.3.2",
    "axios": "^1.4.0",
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "clone-buffer": "^1.0.0",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "mongoose": "^7.0.1",
    "nestjs-typegoose": "^7.1.38",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "which": "^3.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cron": "^2.0.1",
    "@types/escape-string-regexp": "^2.0.1",
    "@types/express": "^4.17.13",
    "@types/jest": "29.2.4",
    "@types/node": "18.11.18",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.7.4"
  }
}
