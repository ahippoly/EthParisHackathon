{
  "private": true,
  "scripts": {
    "rm": "npx rimraf --glob",
    "reinstall": "yarn rm node_modules && yarn install",
    "clean": "yarn rm ./.env ./dist ./.nuxt ./**/*.log ./**/coverage",
    "lint": "eslint --ext \".ts,.js,.vue\" --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext \".ts,.js,.vue\" --ignore-path .gitignore .",
    "dev:local": "bash ./scripts/set_env.bash development && set \"NODE_ENV=development\" && nuxt dev",
    "dev:staging": "bash ./scripts/set_env.bash staging && set \"NODE_ENV=development\" && nuxt dev",
    "dev:prod": "bash ./scripts/set_env.bash production && set \"NODE_ENV=development\" && nuxt dev",
    "test:unit": "bash ./scripts/set_env.bash development && vitest --coverage",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "generate:staging": "bash ./scripts/set_env.bash staging && set \"NODE_ENV=staging\" && nuxt generate",
    "generate:production": "bash ./scripts/set_env.bash production && set \"NODE_ENV=production\" && nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@nuxt/test-utils": "^3.2.0",
    "@nuxtjs/eslint-config": "^12.0.0",
    "@pinia-plugin-persistedstate/nuxt": "^1.0.0",
    "@types/intl": "^1.2.0",
    "@types/node": "^18.11.18",
    "@typescript-eslint/parser": "^5.51.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/coverage-c8": "^0.28.5",
    "@vue/eslint-config-standard": "^8.0.1",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.2.10",
    "add": "^2.0.6",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-nuxt": ">=4.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vitest": "^0.0.34",
    "eslint-plugin-vue-libs": "^4.0.0",
    "jsdom": "^21.1.0",
    "lint-staged": "^13.1.0",
    "nuxt": "^3.2.0",
    "prettier": "^1.16.4",
    "rimraf": "^5.0.0",
    "unplugin-auto-import": "^0.14.2",
    "vitest": "^0.28.5",
    "vue-eslint-parser": "^9.1.0",
    "yarn": "^1.22.19"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-brands-svg-icons": "^6.4.0",
    "@fortawesome/free-regular-svg-icons": "^6.3.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/vue-fontawesome": "^3.0.3",
    "@mdi/font": "^7.2.96",
    "@pinia/nuxt": "^0.4.6",
    "@pinia/testing": "^0.0.14",
    "@sismo-core/sismo-connect-client": "^0.0.19",
    "@stripe/stripe-js": "^1.52.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@wagmi/core": "^1.3.8",
    "@xmtp/xmtp-js": "^10.0.0",
    "buffer": "^6.0.3",
    "ethers": "^6.6.4",
    "pinia": "^2.0.29",
    "sass": "^1.58.0",
    "stripe": "^11.16.0",
    "typescript": "^4.9.5",
    "vagmi": "^0.1.20",
    "viem": "^1.3.1",
    "vuetify": "^3.3.9"
  },
  "lint-staged": {
    "**/*.{js,ts,vue,html,css}": [
      "yarn lintfix"
    ]
  }
}
